@model IEnumerable<MigracionApp.Controllers.VIAJES_TRANSITO_MIGRACION>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Crear Nuevo Viaje", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.destino)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.origen)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tipo_solicitud)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.motivo_viaje)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PASAJEROS.nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.USUARIOS.nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.destino)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.origen)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tipo_solicitud)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.motivo_viaje)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PASAJEROS.nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.USUARIOS.nombre)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.id }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.id }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.id })
        </td>
    </tr>
}

</table>
